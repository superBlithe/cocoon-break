{
  "name": "phaser-game",
  "version": " 0.0.1",
  "description": "Phaser 3 starter template with TypeScript and webpack.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.dev.js",
    "build": "rimraf dist && webpack --config webpack/webpack.prod.js",
    "bundle": "npm run build",
    "serve": "serve dist",
    "format": "prettier --check src/scripts/**",
    "format:write": "prettier --write src/scripts/**"
  },
  "keywords": [
    "Phaser"
  ],
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "devDependencies": {
    "@yandeu/prettier-config": "^0.0.1",
    "copy-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.2.0",
    "javascript-obfuscator": "^2.9.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.3",
    "webpack-obfuscator": "^3.2.0",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "dependencies": {
    "phaser": "^3.53.1"
  }
}
